{
    "name": "jscustom/laravel-user-management",
    "description": "Laravel package for user management. Includes user, user profile and user role.",
    "type": "library",
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JSCustom\\LaravelUserManagement\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jediael Saliba",
            "email": "jscustom@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "JSCustom\\LaravelUserManagement\\Providers\\LaravelUserManagementServiceProvider"
            ],
            "aliases": {
                "Qbo": "JSCustom\\LaravelUserManagement\\Facade"
            }
        }
    }
}
